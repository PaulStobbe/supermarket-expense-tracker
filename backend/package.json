{
  "name": "supermarket-expense-tracker-backend",
  "version": "1.0.0",
  "description": "Backend API for Supermarket Expense Tracker",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "db:migrate": "ts-node src/utils/migrate.ts migrate",
    "db:rollback": "ts-node src/utils/migrate.ts rollback",
    "db:seed": "ts-node src/utils/seed.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "keywords": [
    "expense-tracker",
    "supermarket",
    "nodejs",
    "express",
    "typescript",
    "oauth"
  ],
  "author": "Paul Stobbe",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "express-session": "^1.17.3",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "jsonwebtoken": "^9.0.0",
    "bcryptjs": "^2.4.3",
    "sqlite3": "^5.1.6",
    "knex": "^2.4.2",
    "cors": "^2.8.5",
    "helmet": "^6.1.5",
    "dotenv": "^16.0.3",
    "express-validator": "^6.15.0",
    "morgan": "^1.10.0",
    "compression": "^1.7.4",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.7",
    "@types/passport": "^1.0.12",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/passport-jwt": "^3.0.8",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/morgan": "^1.9.4",
    "@types/compression": "^1.7.2",
    "@types/multer": "^1.4.7",
    "@types/nodemailer": "^6.4.7",
    "@types/uuid": "^9.0.1",
    "@types/node": "^18.16.3",
    "@types/jest": "^29.5.1",
    "@types/supertest": "^2.0.12",
    "supertest": "^6.3.3",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "eslint": "^8.39.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}